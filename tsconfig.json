{
  "compilerOptions": {
    "target": "ES2023",
    "module": "NodeNext",
    "checkJs": true,
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "exclude": ["stylelint"]
}
